@article{Pnarbas2017,
abstract = {Evidence-based decision making is an essential process for sustainable, effective, and efficient marine spatial planning (MSP). In that sense, decision support tools (DSTs) could be considered to be the primary assistant of planners. Although there are many DSTs listed in tool databases, most of them are conceptual and not used in real MSP implementation. The main objective of this review is to: (i) characterize and analyse the present use of the DSTs in existing MSP implementation processes around the world, (ii) identify weaknesses and gaps of existing tools, and (iii) propose new functionalities both to improve their feasibility and to promote their application. In total, 34 DSTs have been identified in 28 different MSP initiatives with different levels of complexity, applicability and usage purposes. Main characteristics of the tools were transferred into a DST matrix. It was observed that limited functionality, tool stability, consideration of economic and social decision problems, ease of use, and tool costs could be considered as the main gaps of existing DSTs. Future developments are needed and should be in the direction of the specific need of marine planners and stakeholders. Results revealed that DST developments should consider both spatial and temporal dynamics of the ocean, and new tools should provide multi-functionality and integrity; meanwhile they should be easy to use and freely available. Hence, this research summarised current use, gaps, and expected development trends of DSTs and it concludes that there is still a big potential of DST developments to assist operational MSP processes.},
author = {Pınarbaşı, Kemal and Galparsoro, Ibon and Borja, {\'{A}}ngel and Stelzenm{\"{u}}ller, Vanessa and Ehler, Charles N. and Gimpel, Antje},
doi = {10.1016/j.marpol.2017.05.031},
file = {:C$\backslash$:/Users/stolte/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Pınarbaşı et al. - 2017 - Decision support tools in marine spatial planning Present applications, gaps and future perspectives.pdf:pdf},
isbn = {0308597X},
issn = {0308597X},
journal = {Marine Policy},
keywords = {Management plan,Marine spatial planning,Maritime activities,Scenarios,Spatial use conflicts,Spatially explicit tools,waddenzee,zeespiegel},
mendeley-tags = {waddenzee,zeespiegel},
number = {February},
pages = {83--91},
title = {{Decision support tools in marine spatial planning: Present applications, gaps and future perspectives}},
volume = {83},
year = {2017}
}
@article{Pitcher2010a,
abstract = {The upwelling systems of the eastern boundaries of the world's oceans are susceptible to harmful algal blooms (HABs) because they are highly productive, nutrient-rich environments, prone to high-biomass blooms. This review identifies those aspects of the physical environment important in the development of HABs in upwelling systems through description and comparison of bloom events in the Benguela, California and Iberia systems. HAB development is dictated by the influence of wind stress on the surface boundary layer through a combination of its influence on surface mixed-layer characteristics and shelf circulation patterns. The timing of HABs is controlled by windstress fluctuations and buoyancy inputs at the seasonal, event and interannual scales. Within this temporal framework, various mesoscale features that interrupt typical upwelling circulation patterns, determine the spatial distribution of HABs. The inner shelf in particular provides a mosaic of shifting habitats, some of which favour HABs. Changes in coastline configuration and orientation, and bottom topography are important in determining the distribution of HABs through their influence on water stratification and retention. A spectrum of coastline configurations, including headlands, capes, peninsulas, R{\'{i}}as, bays and estuaries, representing systems of increasing isolation from the open coast and consequent increasing retention times, are assessed in terms of their vulnerability to HABs.},
author = {Pitcher, G C and Figueiras, F G and Hickey, B M and Moita, M T},
doi = {10.1016/j.pocean.2010.02.002},
file = {:C$\backslash$:/Users/stolte/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Pitcher et al. - 2010 - Progress in Oceanography The physical oceanography of upwelling systems and the development of harmful algal blo.pdf:pdf},
issn = {0079-6611},
journal = {Progress in oceanography},
keywords = {primary production,zeespiegel},
mendeley-tags = {primary production,zeespiegel},
month = {jan},
number = {1-2},
pages = {5--32},
pmid = {22053120},
publisher = {Elsevier Ltd},
title = {{The physical oceanography of upwelling systems and the development of harmful algal blooms.}},
url = {http://dx.doi.org/10.1016/j.pocean.2010.02.002 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3205352{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {85},
year = {2010}
}
