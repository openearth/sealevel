issn = {00777579},
journal = {Netherlands Journal of Sea Research},
keywords = {Eastern Scheldt,Oosterschelde,primary production,waddenzee,zeespiegel},
mendeley-tags = {Eastern Scheldt,Oosterschelde,primary production,waddenzee,zeespiegel},
month = {dec},
