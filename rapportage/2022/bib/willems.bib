file = {:C$\backslash$:/Users/stolte/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Van de Waal et al. - 2015 - Characterization of multiple isolates from an Alexandrium ostenfeldii bloom in The Netherlands.pdf:pdf},
