journal = {Estuarine, Coastal and Shelf Science},
keywords = {GAM modelling,Remane model,Scheldt estuary,Westerschelde,beta diversity,ecocline,ecotone,primary production,salinity,zeespiegel},
mendeley-tags = {Westerschelde,primary production,zeespiegel},
number = {2},
pages = {335--340},
publisher = {Elsevier Ltd},
title = {{Changes in phytoplankton diversity and community composition along the salinity gradient of the Schelde estuary (Belgium/The Netherlands)}},
