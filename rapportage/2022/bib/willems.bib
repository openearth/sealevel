issn = {13652486},
journal = {Global Change Biology},
keywords = {North Sea,bottom-up effects,climate change,fish recruitment,nutrients,phytoplankton,primary production,waddenzee,zeespiegel},
mendeley-tags = {primary production,waddenzee,zeespiegel},
number = {1},
pages = {e352--e364},
pmid = {28944532},
title = {{A decline in primary production in the North Sea over 25 years, associated with reductions in zooplankton abundance and fish stock recruitment}},
