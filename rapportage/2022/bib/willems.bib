issn = {09247963},
journal = {Journal of Marine Systems},
keywords = {Chlorophyll-a,Coastal waters,Eastern Scheldt,Grevelingen,Nutrient reduction,Oosterschelde,Phytoplankton modelling,Water Framework Directive,Westerschelde,primary production,waddenzee,zeespiegel},
mendeley-tags = {Eastern Scheldt,Grevelingen,Oosterschelde,Westerschelde,primary production,waddenzee,zeespiegel},
month = {jan},
number = {1-4},
pages = {201--215},
title = {{Application of a validated primary production model (BLOOM) as a screening tool for marine, coastal and transitional waters}},
