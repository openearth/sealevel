issn = {10543139},
journal = {ICES Journal of Marine Science},
keywords = {Coastal eutrophication,Eastern Scheldt,Ecological efficiency,Mesocosm experiments,Model simulation,Nutrients (phosphorus, nitrogen, N:P ratio),Oosterschelde,Productivity,Riverine inputs,Trend analysis,Westerschelde,primary production,waddenzee,zeespiegel},
