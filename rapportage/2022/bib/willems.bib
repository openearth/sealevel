journal = {Harmful Algae},
keywords = {Allelochemical potency,Eastern Scheldt,Grevelingen,Grevelingenmeer,Gymnodimine,Harmful algal blooms,Lytic activity,Oosterschelde,PSP toxins,Spirolide,zeespiegel},
mendeley-tags = {Eastern Scheldt,Grevelingen,Grevelingenmeer,Oosterschelde,zeespiegel},
