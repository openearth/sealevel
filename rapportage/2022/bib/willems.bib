journal = {Nature},
keywords = {Animals,Diatoms,Diatoms: physiology,Ecosystem,Fishes,Fishes: growth {\&} development,Fishes: physiology,Food Chain,Greenhouse Effect,Marine Biology,Plankton,Plankton: physiology,Seasons,Seawater,Temperature,primary production,zeespiegel},
