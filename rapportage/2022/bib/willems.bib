keywords = {primary production,zeespiegel},
mendeley-tags = {primary production,zeespiegel},
number = {2-3},
pages = {243--249},
pmid = {17520940},
title = {{Management options and effects on a marine ecosystem: Assessing the future of the baltic}},
volume = {36},
year = {2007}
}
