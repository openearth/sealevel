issn = {00218901},
journal = {Journal of Applied Ecology},
keywords = {Coastal ecology,Coastal environmental impact,Eastern Scheldt,Enhancement,Habitat disturbance,Oosterschelde,Resilience,Stability,zeespiegel},
mendeley-tags = {Eastern Scheldt,Oosterschelde,zeespiegel},
