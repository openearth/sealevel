issn = {1725-0463},
journal = {International Journal of Spatial Data Infrastructure Research},
keywords = {Chemical pollution,DPSIR,EUROGEOSS,Ecosystem Approach,Environmental Management,Marine Strategy Framework Directive,Spatial Data Infrastructures,Web Services,zeespiegel},
mendeley-tags = {zeespiegel},
pages = {323--351},
title = {{A multidisciplinary Spatial Data Infrastructure for the Mediterranean to support the implementation of the Marine Strategy Framework Directive }},
volume = {7},
