issn = {00188158},
journal = {Hydrobiologia},
keywords = {1204406,Eastern Scheldt,GEM,Generic ecological model,Nutrients,Oosterschelde,Phytoplankton modelling,Validation,Water quality,Westerschelde,primary production,veersemeer,waddenzee,zeespiegel},
mendeley-tags = {1204406,Eastern Scheldt,Oosterschelde,Westerschelde,primary production,veersemeer,waddenzee,zeespiegel},
