issn = {09247963},
journal = {Journal of Marine Systems},
keywords = {Eastern Scheldt,Ecological modeling,Model validation,North Sea,Oosterschelde,Primary production,Westerschelde,primary production,waddenzee,zeespiegel},
mendeley-tags = {Eastern Scheldt,Oosterschelde,Westerschelde,primary production,waddenzee,zeespiegel},
