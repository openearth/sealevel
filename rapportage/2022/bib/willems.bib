issn = {00777579},
journal = {Netherlands Journal of Sea Research},
keywords = {Eastern Scheldt,Oosterschelde,Westerschelde,primary production,zeespiegel},
mendeley-tags = {Eastern Scheldt,Oosterschelde,Westerschelde,primary production,zeespiegel},
number = {3},
pages = {267--279},
title = {{Primary production of phytoplankton in a turbid coastal plain estuary, the Westerschelde (The Netherlands)}},
url = {http://www.sciencedirect.com/science/article/pii/007775799390027P},
