keywords = {Artificial structures,Natural shores,Southeast Asia,Tropics,zeespiegel},
mendeley-tags = {zeespiegel},
pages = {340--352},
publisher = {Elsevier B.V.},
title = {{Marine habitats and biodiversity of Singapore's coastal waters: A review}},
