issn = {22967745},
journal = {Frontiers in Marine Science},
keywords = {Ocean observation networks,data collection,data gaps,fisheries,oceanography,primary production,science-industry cooperation,shelf seas,vessels of opportunity,zeespiegel},
mendeley-tags = {primary production,zeespiegel},
number = {October},
title = {{Fishing Gear as a Data Collection Platform: Opportunities to Fill Spatial and Temporal Gaps in Operational Sub-Surface Observation Networks}},
volume = {7},
