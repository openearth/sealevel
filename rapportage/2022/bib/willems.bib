file = {:C$\backslash$:/Users/stolte/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Perrin et al. - 2016 - Growth of the coccolithophore {\&}lti{\&}gtEmiliania huxleyi{\&}lti{\&}gt in light- and nutrient-limited reactors relevance f.pdf:pdf},
issn = {1810-6285},
journal = {Biogeosciences Discussions},
keywords = {primary production,zeespiegel},
