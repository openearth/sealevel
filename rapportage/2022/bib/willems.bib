mendeley-tags = {primary production,zeespiegel},
pages = {44--51},
publisher = {Elsevier},
title = {{The EU legal framework for the management of marine complex social-ecological systems}},
url = {http://dx.doi.org/10.1016/j.marpol.2014.11.025},
