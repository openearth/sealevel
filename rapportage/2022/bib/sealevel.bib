@Article{Wada2010,
  author = {Yoshihide Wada and Ludovicus P. H. {van Beek} and Cheryl M. {van Kempen} and Josef W. T. M. Reckman and Slavek Vasak and Marc F. P. Bierkens},
  annote = {doi: 10.1029/2010GL044571},
  booktitle = {Geophysical Research Letters},
  da = {2010/10/01},
  date = {2010/10/26},
