@Article{Cleveland1979,
  author = {William S. Cleveland},
  abstract = {The visual information on a scatterplot can be greatly enhanced, with little additional cost, by computing and plotting smoothed points. Robust locally weighted regression is a method for smoothing a scatterplot, (x<sub>i</sub>, y<sub>i</sub>)},
