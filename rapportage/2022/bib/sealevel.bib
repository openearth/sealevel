@Article{Kemp2011,
  author = {Andrew C. Kemp and Benjamin P. Horton and Jeffrey P. Donnelly and Michael E. Mann and Martin Vermeer and Stefan Rahmstorf},
  abstract = {We present new sea-level reconstructions for the past 2100\textasciitilde y based on salt-marsh sedimentary sequences from the US Atlantic coast. The data from North Carolina reveal four phases of persistent sea-level change after correction for glacial isostatic adjustment. Sea level was stable from at least BC 100 until AD 950. Sea level then increased for 400\textasciitilde y at a rate of 0.6\textasciitilde mm/y, followed by a further period of stable, or slightly falling, sea level that persisted until the late 19th century. Since then, sea level has risen at an average rate of 2.1\textasciitilde mm/y, representing the steepest century-scale increase of the past two millennia. This rate was initiated between AD 1865 and 1892. Using an extended semiempirical modeling approach, we show that these sea-level changes are consistent with global temperature for at least the past millennium.},
  date-added = {2018-09-01 15:57:56 +0200},
  date-modified = {2018-09-01 15:57:58 +0200},
  doi = {10.1073/pnas.1015619108},
