	da = {2020/03/01},
	date-added = {2020-04-14 21:15:45 +0200},
	date-modified = {2020-04-14 21:15:49 +0200},
	doi = {10.1038/s41586-019-1855-2},
	id = {Shepherd2020},
	isbn = {1476-4687},
	journal = {Nature},
	number = {7798},
	pages = {233--239},
	title = {Mass balance of the Greenland Ice Sheet from 1992 to 2018},
