	journal = {Annual Review of Earth and Planetary Sciences},
	number = {1},
	pages = {111--149},
	title = {Global glacial isostacy and the surface of the ice-age earth: The ICE-5G (VM2) model and GRACE},
	url = {http://www.annualreviews.org/doi/abs/10.1146/annurev.earth.32.082503.144359},
	volume = {32},
	year = {2004},
