	institution = {Joint Research Centre (Seville site)},
	keywords = {Floods; inundation; damage functions; global},
	month = Apr,
	number = {JRC105688},
	title = {{Global flood depth-damage functions: Methodology and the database with guidelines}},
	type = {JRC Working Papers},
	url = {https://ideas.repec.org/p/ipt/iptwpa/jrc105688.html},
	year = 2017,
