	editor = {Douglas, Bruce C. and Kearney, Michael S. and Leatherman, Stephen P.},
	isbn = {0074-6142},
	pages = {65--95},
	publisher = {Academic Press},
	title = {Chapter 4 Global glacial isostatic adjustment and modern instrumental records of relative sea level history},
