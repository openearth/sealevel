	issn = {0169-555X},
	journal = {Geomorphology},
	keywords = {Dutch coast},
	number = {0},
	pages = {-},
	title = {Assessment of dune failure along the Dutch coast using a fully probabilistic approach},
	url = {http://www.sciencedirect.com/science/article/pii/S0169555X11004806},
	year = {2011},
	bdsk-file-1 = {},
