@article{McKay2011,
	annote = {doi: 10.1029/2011GL048280},
	author = {McKay, Nicholas P. and Overpeck, Jonathan T. and Otto-Bliesner, Bette L.},
	booktitle = {Geophysical Research Letters},
	da = {2011/07/01},
	date = {2011/07/01},
	date-added = {2019-02-03 15:33:01 +0100},
	date-modified = {2019-02-03 15:33:02 +0100},
	doi = {10.1029/2011GL048280},
	isbn = {0094-8276},
