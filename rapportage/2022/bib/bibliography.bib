	issn = {0378-3839},
	journal = {Coastal Engineering},
	keywords = {Coastal management; jarkus},
	number = {8},
	pages = {749 - 760},
	title = {Data-based yearly forecasting of beach volumes along the Dutch North Sea coast},
	url = {http://www.sciencedirect.com/science/article/pii/S0378383911000469},
	volume = {58},
	year = {2011},
