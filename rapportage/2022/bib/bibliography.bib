	doi = {10.1038/s41558-020-0697-0},
	id = {Vousdoukas2020},
	isbn = {1758-6798},
	journal = {Nature Climate Change},
	number = {3},
	pages = {260--263},
	title = {Sandy coastlines under threat of erosion},
	ty = {JOUR},
	url = {https://doi.org/10.1038/s41558-020-0697-0},
	volume = {10},
	year = {2020},
	bdsk-url-1 = {https://doi.org/10.1038/s41558-020-0697-0}}

@article{Caires2016,
	abstract = {{In order to assess their relative merits in the context of the determination of metocean extremes, the annual maxima (AM) and the peaks over threshold (POT) approaches are compared in terms of their accuracy in estimating exceedance probabilities on the basis of time series with various lengths and with characteristic that mimic those of real time series, such as nonstationarity and serial dependence. Based on the results of this study, the use of the POT approach is recommended. Furthermore, the method of probability weighted moments (PWMs) is recommended for the estimation of the parameters of the generalized Pareto distribution (GPD).}},
