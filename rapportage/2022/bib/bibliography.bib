	journal = {Coastal Engineering},
	keywords = {High-resolution modelling},
	note = {<ce:title>The CoastView Project: Developing coastal video monitoring systems in support of coastal zone management</ce:title>},
	number = {6--7},
	pages = {539 - 553},
	title = {The role of video imagery in predicting daily to monthly coastal evolution},
	url = {http://www.sciencedirect.com/science/article/pii/S0378383907000221},
	volume = {54},
	year = {2007},
	bdsk-file-1 = {},
